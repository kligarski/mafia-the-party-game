{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --emptyOutDir --base /static/frontend/dev --outDir ../backend/staticfiles/frontend/dev",
